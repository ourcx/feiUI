{
    "name": "feiui",
    "version": "1.0.0",
    "description": "A Vue 3 UI component library",
    "type": "module",
    "main": "./dist/feiui.umd.cjs",
    "module": "./dist/feiui.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/feiui.js",
            "require": "./dist/feiui.umd.cjs",
            "types": "./dist/index.d.ts"
        },
        "./style.css": "./dist/style.css"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "keywords": [
        "vue3",
        "ui",
        "components",
        "frontend",
        "feiui"
    ],
    "homepage": "https://github.com/your-username/feiui",
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/feiui.git"
    },
    "bugs": {
        "url": "https://github.com/your-username/feiui/issues"
    },
    "scripts": {
        "build:lib": "vite build --config vite.lib.config.ts",
        "prepublishOnly": "npm run build:lib",
        "type-check": "vue-tsc --build --force"
    },
    "peerDependencies": {
        "vue": "^3.5.0"
    },
    "dependencies": {
        "@floating-ui/vue": "^1.1.6",
        "@fortawesome/fontawesome-svg-core": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@fortawesome/vue-fontawesome": "^3.0.8",
        "async-validator": "^4.2.5",
        "lodash-es": "^4.17.21"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.2.3",
        "@vitejs/plugin-vue-jsx": "^4.1.2",
        "typescript": "~5.8.0",
        "vite": "^6.2.4",
        "vue-tsc": "^2.2.8"
    },
    "license": "MIT"
}